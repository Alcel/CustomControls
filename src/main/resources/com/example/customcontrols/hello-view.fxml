<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<?import com.example.customcontrols.CombinedControl?>
<?import com.example.customcontrols.CustomControl?>
<?import com.example.customcontrols.RegionControl?>
<?import com.example.customcontrols.CanvasControl?>
<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.customcontrols.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <RegionControl></RegionControl>
   <HBox prefHeight="100.0" prefWidth="200.0" />
    <CanvasControl></CanvasControl>
    <CombinedControl></CombinedControl>
    <CustomControl></CustomControl>
    <Label fx:id="welcomeText" />
   <CheckBox mnemonicParsing="false" text="CheckBox" />
    <Button onAction="#onHelloButtonClick" text="Hello!" />
</VBox>
